<html>

<head>
  <title>User List</title>
</head>

<body>
  <h1>User List</h1>
  <table border="1" cellpadding="5">
    <thead>
      <tr>
        <th>ID</th>
        <th>Username</th>
        <th>Email</th>
        <th>Created At</th>
      </tr>
    </thead>
    <tbody>
      {{#each users}}
      <tr>
        <td>{{this.id}}</td>
        <td>{{this.username}}</td>
        <td>{{this.email}}</td>
        <td>{{this.createdAt}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
  <a href="/">Back to Home</a>
</body>

</html>